/* pages/space/space.wxss */
page{
  height: 100%;
}
.top{
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1001;
}
.search{
  width: 100%;
  height: 100rpx;
  background: var(--c);
}
.search .left{
  width: 220rpx;
  height: 100%;
  float: left;
}
.search .left::after{
  display: inline-block;
  content: "";
  height: 24rpx;
  width: 24rpx;
  padding-left: 20rpx;
  background: url(data:image/png;base64,);
  background-size: contain;
  background-position: 50%;
  background-repeat: no-repeat;
  vertical-align: middle;
  transition: all .3s;
  box-sizing: initial;
}
.search .left .icon .topPosition{
  width: 44rpx;
  height: 48rpx;
  float: left;
  margin: 26rpx 20rpx 0 20rpx;

}
.search .left .city{
  display: inline-block;
  color: #fff;
  line-height: 100rpx;
  font-size: 28rpx;
}
.search .right{
  display: inline-block;
  width: 480rpx;
  height: 100%;
  margin-left: 30rpx;
  position: relative;
}
.search .right .icon{
  position: absolute;
  top: 30rpx;
  left: 20rpx;
}
.search .right .icon image{
  width: 40rpx;
  height: 40rpx;
}
.search .right .search-box .search-input{
  width: 100%;
  height: 70rpx;
  background: #fff;
  border-radius: 50rpx;
  vertical-align: middle;
  margin-top: 16rpx;
  font-size: 24rpx;
  padding-left: 70rpx;
  box-sizing: border-box;
}
.nav-bar{
  width: 100%;
  height: 100rpx;
  background: #fff;
  border-bottom: 1px solid #f2f2f2;
  box-sizing: border-box;
  /* box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.2); */
}
.nav-bar .select-box{
  width: 50%;
  display: inline-block;
}
.nav-bar .select-box .title{
  line-height: 100rpx;
  text-align: center;
  color: #4A4A4A;
  font-size: 28rpx;
}
.nav-bar .select-box .title::after{
  display: inline-block;
  content: "";
  height: 24rpx;
  width: 24rpx;
  padding-left: 20rpx;
  background: url(data:image/png;base64,);
  background-size: contain;
  background-position: 50%;
  background-repeat: no-repeat;
  vertical-align: middle;
  transition: all .3s;
  box-sizing: initial;
}
.box-1{
  height: 450rpx;
  background-color: #fff;
}
.result-box{
  width: 100%;
  height: calc(100% - 200rpx);
  position: absolute;
  top: 200rpx;
  overflow-y: scroll;
  box-sizing: border-box;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/* .result-box scroll-view{
  height: 100%;
} */
.result-box .loadingComplete{
  text-align: center;
  line-height: 60rpx;
  color: #9b9b9b;
}
.result-box .item{
  padding: 24rpx;
  box-sizing: border-box;
  display: inline-block;
  width: 100%;
  border-bottom: 1px solid #f2f2f2;
}
.result-box .item .hotel-img{
  display: block;
  width: 170rpx;
  height: 230rpx;
  float: left;
}
.result-box .item .hotel-info{
  display: inline-block;
  padding-left: 30rpx;
  position: relative;
  height: 230rpx;
  overflow: hidden;
}
.no-result{
  text-align: center;
  position: relative;
  top: 50%;
  line-height: 40rpx;
  margin-top: -30rpx;
}
.result-box .no-result .col_4a{
  color: #4a4a4a;
  font-size: 28rpx;
}
.result-box .no-result .col_9b{
  color: #9b9b9b;
  font-size: 24rpx;
}
.item .hotel-info .name{
  font-size: 28rpx;
  font-weight: bold;
  color: #4a4a4a;
}
.item .hotel-info .h-tag{
  line-height: 22px;
  color: #F75141;
  padding: 0 10px;
  vertical-align: middle;
  display: inline-block;
}
.item .hotel-info .item-address{
  line-height: 24rpx;
  color: #4a4a4a;
  width: 405rpx;
  padding: 12rpx 0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.item-detail{
  width: 500rpx;
}
.item-detail .detail-item{
  display: inline-block;
  width: 250rpx;
  line-height: 50rpx;
}
.item-detail .detail-item .item-title{
  color: #9B9B9B;
  padding-right: 10rpx;
}
.item-detail .detail-item .item-value{
  color: #4a4a4a;
}
.item-detail .detail-item .item-value.col_F7{
  color: #F75141;
}
.van-hairline--top-bottom::after{
  border:0 !important
}
.moseiu{
  background: var(--c)
}
.van-tree-select__selected{
  display: none;
}
.van-sidebar-item--selected{
  border-color: #fff !important;
}

.van-tree-select__item:first-child{
  color: #F75141;
}