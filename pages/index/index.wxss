/**index.wxss**/
page{
  background: var(--c);
}
.logo{
  /* width: 297rpx; */
  /* height: 105rpx; */
  width: 198rpx;
  height: 70rpx;
  margin: 40rpx auto;
}
.form{
  padding: 40rpx;
  width: 100%;
  display: block;
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  margin-top: -600rpx;
}
.form-info{
  width: 100%;
  height: 100%;
  background: #fff;
  border-radius: 20rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.form-info .title{
  text-align: center;
  padding: 34rpx 0 18rpx 0;
  font-size: 40rpx;
  color: var(--c)
}
.form-info .item-box{
  border: 1px solid #DCDFE6;
  height: 80rpx;
  margin-bottom: 18rpx;
  border-radius: 10rpx;
  position: relative;
  display: block;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
}
.form-info .item-box::after{
  position: absolute;
  content: "";
  height: 24rpx;
  width: 24rpx;
  padding-left: 10rpx;
  background: url(data:image/png;base64,);
  background-size: contain;
  background-position: 50%;
  background-repeat: no-repeat;
  transition: all .3s;
  right: 20rpx;
  top: 30rpx;
  
}
.item-box .icon{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 50%;
  margin-top: -25rpx;
  left: 20rpx;
}
.item-box .em{
  position: absolute;
  top: 20rpx;
  left: 80rpx;
  color: red;
}
.item-box .value{
  width: 100%;
  font-size: 28rpx;
  line-height: 80rpx;
  color: #000;
  display: inline-block;
  overflow: hidden;
  padding-left: 100rpx;
  box-sizing: border-box;
}
.btn-box{
  margin-bottom: 40rpx;
}
.item-box.item-date{
  margin-right: 4%;
}
.item-box.item-date,.item-box.item-long{
  width: 48%;
}
.van-enter-active-class,
.van-leave-active-class {
  transition-property: background-color, transform;
}
.van-enter-class,
.van-leave-to-class {
  background-color: #000;
}
.showni{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #fff;
  z-index: 999;
}
.van-popup--bottom{
  bottom: 96rpx !important;
}
.van-action-sheet__item, .van-action-sheet__cancel {
  line-height: 96rpx !important;
}
.van-action-sheet {
  max-height: var(--action-sheet-max-height,50%) !important;
}
  