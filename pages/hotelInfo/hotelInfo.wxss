/* pages/hotelInfo/hotelInfo.wxss */
.topBanner{
  width: 100%;
  height: 460rpx;
}
.info{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  float: left;
  font-size: 28rpx;
  position: relative;
}
.info .title{
  font-weight: bold;
  font-size: 48rpx;
  padding-top: 20rpx;
}
.info .info-item{
  line-height: 50rpx;
}
.info-item .tag{
  width: 160rpx;
  float: left;
}
.info-item .score{
  display: inline-block;
  color: #4a4a4a;
}
.info-item.item1{
  padding: 10rpx 0;
}
.info-item .score .inline{
  top: 4rpx;
  padding-left: 10rpx;
  display: inline-block;
  position: relative;
  margin-left: -20rpx;
}
.info-item .score .rate,.info-item .score .em{
  display: block;
  width: 87px;
  height: 16px;
  overflow: hidden;
  background: url(data:image/png;base64,) -3px -18px no-repeat;
}
.em{
  position: absolute;
  top: 0;
  left: 0;
  background-position: 4rpx -2px !important;
  width: 100rpx;
}
.info .other{
  position: absolute;
  top: 65rpx;
  right: 0;
  font-size: 28rpx;
  color: #4a4a4a;
}
.info .other .collect{
  width: 130rpx;
  height: 84rpx;
  text-align: center;
  border-left: 1px solid #f2f2f2;
  padding: 10rpx;
  box-sizing: border-box;
}
.info .other .collect .icon-like{
  width: 32rpx;
  height: 32rpx;
  display: block;
  margin: 0 auto;
}
.info .other .skeleton{
  margin-top: 18rpx;
  text-align: right;
  padding-right: 24rpx;
}
.tags, .detail, .pkg, .about{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  float: left;
  position: relative;
}
.tags .tag-item{
  display: inline-block;
  padding: 8rpx 16rpx;
  color: #7D3F96;
  line-height: 24rpx;
  margin: 8rpx 15rpx 8rpx 0;
  border: 1px solid #7D3F96;
}
.detail{
  display: flex;
  text-align: center;
  justify-content: space-between;
}
.detail .detail-item{
  padding: 20rpx 0;
}
.detail .detail-item .img{
  width: 84rpx;
  height: 84rpx;
  margin: 0 auto;
}
.detail .detail-item .msg{
  font-size: 28rpx;
  line-height: 40rpx;
  padding-top: 10rpx;
}
.detail .detail-item .msg .fs12{
  font-size: 24rpx;
  color: #6E6A6A;
}
.pkg{
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  color: #fff;
}
.pkg .pkg-item{
  width: 224rpx;
  background: var(--c);
  padding: 15rpx 0;
  border-radius: 10rpx;
  text-align: center;
}
.about{
  font-size: 28rpx;
}
.about .title{
  font-size: 32rpx;
  font-weight: bold;
  border-left: 10rpx solid var(--c);
  padding-left: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}
.about .more{
  line-height: 45rpx;
  margin-bottom: 30rpx;
}

.swiperBox {
  width: 100%;
  height: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.swiperBox .title {
  width: 100%;
  height: 90rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #EDEDED;
}
.swiperBox .bgf{
  background: #fff;
}
.swiperBox .titleSel {
  flex: 1;
  color: #4a4a4a;
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
}
.swiperBox .titleSel text{
  line-height: 80rpx;
}
.swiperBox .titleUnsel {
  color: #858fab;
  font-size: #858fab;
}

.swiperBox .headerLineSel {
  background: var(--c);
  height: 6rpx;
  width: 100%;
  position: relative;
}

.swiperBox .headerLineUnsel {
  background: #EDEDED;
  height: 6rpx;
  width: 100%;
  position: relative;
}

.swiperBox .swiper {
  width: 100%;
  flex: 1;
  height: 1100rpx;
  overflow: scroll;
  box-sizing: border-box;
}

.swiperBox .recordItem {
  margin-top: 10rpx;
  background-color: white;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
}
.meetInfo{
  font-size: 24rpx;
  color: #4a4a4a;
  padding: 30rpx;
  height: 100%;
  box-sizing: border-box;
}
.meetInfo .info-items{
  height: 100%;
  overflow: scroll;
}
.meetInfo .meet-device{
  color: var(--c);
  padding-bottom: 20rpx;
}
.meetInfo .info-items .info-item{
  display: flex;
  line-height: 45rpx;
  padding: 20rpx 0;
  border-bottom: 1px solid #f2f2f2;
}
.meetInfo .info-items .info-item.info-room .item{
  width: 410rpx;
}
.meetInfo .info-items .info-item.info-room .item .w140{
  width: 160rpx;
  margin-right: 10rpx;
  display: block;
  float: left;
}
.meetInfo .info-items .info-item .item{
  width: 300rpx;
}
.meetInfo .info-items .info-item .name{
  font-size: 28rpx;
  font-weight: bold;
}
.meetInfo .info-items .info-item .pri{
  line-height: 140rpx;
}
.meetInfo .info-items .info-item .pri .col_tag{
  font-size: 28rpx;
}
.meetInfo .info-items .info-item .pre{
  flex: 1;
}
.meetInfo .info-items .info-item .pre .btn{
  background: #F75141;
  width: 148rpx;
  height: 64rpx;
  font-size: 28rpx;
  line-height: 64rpx;
  float: right;
  margin-top: 40rpx;
}
.callme{
  position: fixed;
  width: 100%;
  right: 0;
  left: 0;
  bottom: 0;
  background: #fff;
  padding: 10rpx 40rpx;
  box-sizing: border-box;
}
.callme .btn{
  background: #F75141;
}